[BOT]
TOKEN=<INSERT-YOUR-TOKEN>
SDXL_SOURCE=LOCAL
MESSAGE_INTERACTION_TIMEOUT=5400

[API]
API_KEY=
API_HOST=https://api.stability.ai

[LOCAL]
SERVER_ADDRESS=127.0.0.1:8188

[LOCAL_TEXT2IMG]
CONFIG=comfyUI-workflows/text2img_config.json
PROMPT_NODES=6,12
NEG_PROMPT_NODES=7,13
RAND_SEED_NODES=17,20

[LOCAL_IMG2IMG]
CONFIG=comfyUI-workflows/img2img_config.json
PROMPT_NODES=2
NEG_PROMPT_NODES=3
RAND_SEED_NODES=4
FILE_INPUT_NODES=5

[LOCAL_UPSCALE]
CONFIG=comfyUI-workflows/upscale_config.json
PROMPT_NODES=
NEG_PROMPT_NODES=
RAND_SEED_NODES=
FILE_INPUT_NODES=1

[API_TEXT2IMG]
ENGINE=stable-diffusion-xl-1024-v1-0
CFG=8
WIDTH=1024
HEIGHT=1024
SAMPLES=4
SAMPLER=K_DPMPP_2S_ANCESTRAL
STEPS=70

[API_IMG2IMG]
ENGINE=stable-diffusion-xl-1024-v1-0
INIT_IMAGE_MODE=IMAGE_STRENGTH
IMAGE_STRENGTH=0.35
CFG=7
SAMPLES=4
SAMPLER=K_DPMPP_2S_ANCESTRAL
STEPS=40

[API_UPSCALE]
ENGINE=esrgan-v1-x2plus
WIDTH=2048
SEED=0
STEPS=50
CFG=7